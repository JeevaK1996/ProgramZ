package inheritance;

class abc{
	int a=0;
	public int add(int x,int y){
		return a=x+y;
	}
	public int sub(int x,int y){
		return a=x-y;
	}
}
public class Exaple extends abc {
public int mul(int x,int y){
	return x*y;
}
public static void main(String[] args) {
	Exaple exaple=new Exaple();
	exaple.add(9, 7);
	exaple.sub(5, 3);
	exaple.mul(3, 3);
}
}
